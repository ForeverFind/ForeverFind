git init
git add .
git commit -m "Initial commit - ForeverFind wedding vendor app"
git remote add origin https://github.com/your-username/foreverfind.git
git branch -M main
git push -u origin main
#!/bin/bash

# Replace with your GitHub username and repo name
GITHUB_USERNAME="your-username"
REPO_NAME="foreverfind"

# Initialize git if not already initialized
if [ ! -d ".git" ]; then
  git init
  echo "Initialized empty Git repository."
else
  echo "Git repository already initialized."
fi

# Add all files and commit
git add .
git commit -m "Initial commit - ForeverFind wedding vendor app"

# Add remote origin
git remote add origin https://github.com/$GITHUB_USERNAME/$REPO_NAME.git

# Set main branch and push
git branch -M main
git push -u origin main

echo "Done! Your code is pushed to https://github.com/$GITHUB_USERNAME/$REPO_NAME"
